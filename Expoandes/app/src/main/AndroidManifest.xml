<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.expoandes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icono_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Expoandes.NoActionBar">
        <activity android:name=".ContactActivity"/>
        <activity android:name=".SignUpActivity"/>
        <activity android:name=".DevolverActivity"/>
        <activity android:name=".InfoConfirmar"/>
        <activity android:name=".PrestamosView"/>
        <activity android:name=".CosasPropiasView"/>
        <activity android:name=".AddActivity"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />

        <activity android:name=".AuthActivity" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>